<script type="text/javascript">
    $(document).ready(function () {

        $("form#formComment").submit(function (e) {
            e.preventDefault();
            $("#comment_save").attr('disabled', 'disabled');

            $('#loader-comment').show();
            var form = $(this);
            $.post(form.attr("action"), form.serialize(),
                function (data) {
                    try {
                        console.log('OK');
                        $('#comments').html(data);
                    } catch (e) {
                        alert(data);
                    }
                });
        });

        $(".replyButton").click(function (e) {
            $("#comment_content").focus();
            $('.commentDetails').removeClass('hide');
        });

        
            });
</script>


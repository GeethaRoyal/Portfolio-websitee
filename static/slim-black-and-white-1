<script type="text/javascript">
    $(document).ready(function () {
        $("#favoritedBy").fancybox({type: 'ajax'});

        var str = window.location.href;

        if (str.indexOf('blog') != -1) {
//window.location.replace("http://artmajeur.com");
        }
        $("#sectionTrigger a").on('click', function (e) {
            e.preventDefault();
            $("#sectionTrigger a").removeClass('active').addClass('inactive');
            $(this).removeClass('inactive').addClass('active');

            $('#printsTabContent, #originalTabContent, #digitalsTabContent').hide();
            $("#" + $(this).prop('id') + "Content").show();

        });

        if (window.location.hash == '#prints') {
            $("#printsTab").click();
        }
        if (window.location.hash == '#digitals') {
            $("#digitalsTab").click();
        }


    });


</script>


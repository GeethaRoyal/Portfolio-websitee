<script type="text/javascript">

    var carouselTo = 4,
        videoItemIndex = 0;

    $(document).ready(function () {
        var carouselImage = $('#carousel_image').owlCarousel({
            items: 1,
            center: true,
            dots: false,
            // autoHeight: true,
            video: true,
            videoHeight: 500,
            lazyLoad: true,
            onChanged: function (event) {

                var property = event.property,
                    propertyName = property.name,
                    propertyValue = property.value,
                    $carouselItemInsitu = $('.carousel-item-insitu');

                if (propertyName == 'position') {
                    var $elem = $('.carousel-thumbnail a[data-carousel-to="' + propertyValue + '"]'),
                        $img = $elem.find('img');

                    $('.carousel-thumbnail img').removeClass('active');
                    $img.addClass('active ');

                    if (propertyValue == videoItemIndex && owlVideoPlaying) {
                        $('div.carousel-item-video').parent().addClass('owl-video-playing');
                    }

                    $carouselItemInsitu.css({'height': '1px'});
                    if (propertyValue == carouselTo) {
                        $carouselItemInsitu.css({'height': 'auto'});
                    }
                }
            },
        });

        $(document).on('click', '[data-carousel-to]', function (e) {
            e.preventDefault();

            var $elem = $(e.currentTarget);

            carouselImage.trigger('to.owl.carousel', [$elem.data('carousel-to')]);
        });

        //
        var carouselZoom = $('#carousel_zoom').owlCarousel({
            items: 1,
            center: true,
            dots: false,
            lazyLoad: true,
            autoHeight: true,
            onChanged: function (event) {
                var property = event.property,
                    propertyName = property.name,
                    propertyValue = property.value;

                if (propertyName == 'position') {
                    $('#modal_zoom .modal-zoom-direction').removeClass('hide');
                    if (propertyValue == 0) {
                        $('#modal_zoom .modal-zoom-direction-left').addClass('hide');
                    }
                    if (propertyValue == nbImageZoom - 1) {
                        $('#modal_zoom .modal-zoom-direction-right').addClass('hide');
                    }
                }
            }

        });

        $(document).on('click', '[data-zoom-to]', function (e) {
            e.preventDefault();

            var $elem = $(e.currentTarget);
            carouselZoom.trigger('to.owl.carousel', [$elem.data('zoom-to')]);

            $('#modal_zoom').modal('show');
        });

        $(document).on('click', '[data-carousel-direction]', function (e) {
            e.preventDefault();

            var direction = $(e.currentTarget).data('carousel-direction');

            switch (direction) {
                case 'left':
                    carouselZoom.trigger('prev.owl.carousel');
                    break;
                case 'right':
                    carouselZoom.trigger('next.owl.carousel');
                    break;
            }
        });

        $(window).on('resize', function (e) {
            var viewWidth = window.innerWidth,
                viewHeight = window.innerHeight,
                $img = $('img.image-in-situ');

            if (viewWidth < viewHeight) {
                $img.css('height', viewWidth + 'px');
            }
        });

        $('#modal_zoom')
            .on('show.bs.modal', function (e) {
                // addThis icon bar
                $('#at4-share').addClass('hide');
                $('#at-share-dock').addClass('hide');

                // wix icon
                $('span#launcher-icon').addClass('hide');
            })
            .on('shown.bs.modal', function (e) {
                $(window).trigger('resize');
            })
            .on('hidden.bs.modal', function (e) {
                // addThis icon bar
                $('#at4-share').removeClass('hide');
                $('#at-share-dock').removeClass('hide');

                // wix icon
                $('span#launcher-icon').removeClass('hide');
            });

        var nbImageZoom = $('#modal_zoom .carousel-item').length;

        var owlVideoPlaying = false;
        $(document).on('click', 'div.carousel-item-video div.owl-video-play-icon', function (e) {
            owlVideoPlaying = true;
        })
    });
</script>                        <div>

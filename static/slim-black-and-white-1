        <script type="text/javascript">
            $(document).ready(function () {
                

                $('[data-toggle=confirmation]').confirmation({
                    rootSelector: '[data-toggle=confirmation]',
                    popout: true,
                    title: 'Are you sure?',
                    btnOkLabel: 'Yes',
                    btnCancelLabel: 'No'
                });

                $(window).on('click', '[data-confirmation]', function (e) {
                    if (!confirmationLink(e.currentTarget)) {
                        e.preventDefault();
                    }
                });

                // checkbox bootstrap switch
                $('div.checkbox-toggle input:checkbox').bootstrapSwitch({
                    onText: 'Yes',
                    offText: 'No',
                    size: 'mini'
                });

                $(':checkbox.checkbox-toggle').bootstrapSwitch({
                    onText: 'Yes',
                    offText: 'No',
                    size: 'mini'
                });

                $(document).on('click', '[data-toggle="confirm-action"]', function (e) {
                    e.preventDefault();

                    var $this = $(this),
                        url = this.href || $this.data('url'),
                        message = $this.data('message');

                    confirmAction(url, message);
                });
            });

            function confirmAction(actionUrl, actionMsg) {
                actionMsg = typeof actionMsg != 'undefined' ? actionMsg : "ATTENTION! ARE YOU SURE?";

                bootbox.confirm({
                    size: 'small',
                    title: 'Action Required',
                    message: actionMsg,
                    locale: 'en',
                    className: 'confirm-action',
                    callback: function (result) {
                        if (result) {
                            if (actionUrl != undefined && actionUrl) {
                                document.location = actionUrl;
                            } else {
                                document.location.reload();
                            }
                        }
                    }
                });
            }

            function confirmationLink(link) {
                var $link = $(link);

                if ($link.data('isConfirmed')) {
                    return true;
                }

                var size = $link.data('confirmation-size') || 'small',
                    title = $link.data('confirmation-title') || 'Action Required',
                    message = $link.data('confirmation-message') || 'ATTENTION! ARE YOU SURE?';

                bootbox.confirm({
                    size: size,
                    title: title,
                    message: message,
                    className: 'confirm-action',
                    buttons: {
                        confirm: {
                            label: 'Yes'
                        },
                        cancel: {
                            label: 'No'
                        }
                    },
                    callback: function (result) {
                        if (result) {
                            $link.data('isConfirmed', true);
                            $link.trigger('click');
                        }
                    }
                });

                return false;
            }

            /*
             *
             */
            $.fn.select2.defaults.set('theme', 'bootstrap');
            $.fn.select2.defaults.set('placeholder', 'Please select');
            $.fn.select2.defaults.set('minimumResultsForSearch', 'Infinity');

        </script>

    

        <script type="text/javascript">
            $(document).ready(function () {
                $('a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
                    var tab = e.target.hash;
                    Cookies.set('artworkTab', tab);
                });

                var cookieTab = Cookies.get('artworkTab');
                if (cookieTab) {
                    $('#artworkTabs a[href="' + cookieTab + '"]').tab('show');
                }

                //
                $('.childDropdown').on('click', function (e) {
                    e.stopPropagation();

                    var $btn = $(e.currentTarget),
                        doOpen = !$btn.data('dropdown-open');

                    if (doOpen) {
                        $btn.parent().addClass('open');
                    } else {
                        $btn.parent().removeClass('open');
                    }

                    $btn.data('dropdown-open', doOpen);
                })
            });
        </script>
    

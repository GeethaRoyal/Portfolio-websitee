<script type="text/javascript">

    function onFollowingLinkFollowedSuccess(event, data) {
        var $link = $('#following_link_' + data.artmajeurId);
        $link.addClass('btn-default')
            .removeClass('btn-primary')
            .removeClass('js-form')
            .html('<i class="fa fa-check "></i> Followed')
            .attr('href', data.route);

        $(':focus').blur();
    };

    function onFollowingLinkLeftSuccess(event, data) {
        var $link = $('#following_link_' + data.artmajeurId);
        $link.addClass('btn-primary')
            .removeClass('btn-default')
            .removeClass('js-form')
            .html('<i class="fa fa-plus "></i> Follow')
            .attr('href', data.route);

        $(':focus').blur();
    };

    $(document).on('following-link.followed', onFollowingLinkFollowedSuccess);
    $(document).on('following-link.left', onFollowingLinkLeftSuccess);

</script>

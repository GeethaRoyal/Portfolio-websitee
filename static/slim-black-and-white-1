<script>
    // Submit on button click behavior
    $('#auto_complete_button').click((e) => {
        e.preventDefault();
        var term = sanitizer.sanitize($('#auto_complete').val());
        if (term) {
            url = url.replace("term", term);
            window.location.href = url;
        }
    });
    var url = '/en/artworks?q=term';
    var input_web = document.getElementById("auto_complete");
    var input_mobile = document.getElementById("auto_complete_mobile");
            if (input_web) autoCompleter(autoCompleterOption(input_web, 'results-web', $('#web_input_container')));

    if (input_mobile) autoCompleter(autoCompleterOption(input_mobile, 'results-mobile', $('#mobile_input_container')));

    /**
     * @see https://github.com/kraaden/autocomplete
     * AutoCompleter Option
     * @Param input              input tag
     * @Param className          autocomplete render block class name
     * @Param resultContainer    jquery element that will contain the results
     * return Object             Option for autoCompleter
     */
    function autoCompleterOption(input, className, resultContainer) {
        return {
            input: input,
            className: className,
            debounceWaitMs: 200,
            fetch:
                function (text, update) {
                    text = sanitizer.sanitize(text).trim();
                    if (!text) {
                        return
                    }
                    /*
                    *   To display "Loading..."
                    *   pending the return of the request
                    *   If the user submits the form before the response, it will be redirected
                    *   to search_artwork route
                    */
                    var loadingUrl = url.replace("term", text);
                    var loading = {loading: true, url: loadingUrl};
                    update([loading]);
                    update(false);
                    var suggestions;
                    $.ajax({
                        type: 'GET',
                        url: '/en/esearch.json',
                        data: {q: text}
                    }).done((data) => {
                        suggestions = [{
                            url: data.fullResultUrl,
                            seeFullResult: data.seeFullResult
                        }, ...data.results];
                        update(suggestions);
                    });
                },
            // Render Templating
            render: (suggestion, currentInput) => {
                var content;
                var reg = new RegExp(currentInput, 'gi');
                const itemElement = document.createElement("div");
                itemElement.classList.add('suggestion_render');

                if (suggestion.loading) {
                    content = '<div class="suggestion_item p-3"><p class="my-3">Loading...</p></div>';
                    itemElement.innerHTML = content;
                    return itemElement;
                }


                if (suggestion.seeFullResult) {
                    content = '<div class="suggestion_item p-3"><p class="my-3"><i class="fa fa-arrow-right"></i> See full results for %s%</p></div>';
                    content = content.replace('%s%', `<span class="h4">${sanitizer.sanitize(input.value)}</span>`);
                } else {


                    content = `
                <div class="d-flex p-3 suggestion_item ${suggestion.type}">
                    <img class="${suggestion.type === 'artist' ? 'img-circle border' : ''}" style="width: 45px; height: 45px;" src=${suggestion.imgUrl} alt=${suggestion.text}">
                    <div class="ml-2 d-flex flex-column">
                        <span class="text-result"> ${suggestion.text.replace(reg, function (optionText) {
                        return `<span class="text-dark font-weight-bold">${optionText}</span>`
                    })}</span>
                        <span class="h6 text-result text-secondary"> ${suggestion.subText.replace(suggestion.artistName, function (optionText) {
                        return optionText.replace(reg, function (boldText) {
                            return `<span class="text-dark s font-weight-bold">${boldText}</span>`
                        })
                    })}</span>
                    </div>
                </div>
            `;
                }

                itemElement.innerHTML = content;
                return itemElement;
            },
            onSelect: (element, autocomplete) => {
                window.location.href = element.url
            },
            emptyMsg: 'No results found.',
            preventSubmit: true,
            showOnFocus: true,
            // Required to set the final position of the results
            // By default result tag is applied at the bottom of the document with fixed style
            // when we want it in the defined tag ( Styles are also customized )
            customize: function (input, inputRect, container, maxHeight) {
                resultContainer.append($(container))
            },
        }

    }
</script>

        
            

        
        <div class="container">
            
                <style type="text/css">
        @media (min-width: 768px) and (max-width: 1200px) {
            .container {
                width: 100%;
            }
        }
    </style>


    


    
